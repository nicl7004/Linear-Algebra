A(:,:,12) = [-1.5;.1;0;];
A(:,:,13) = [-1.5;.2;0;];
A(:,:,14) = [-1.5;.3;0;];
A(:,:,15) = [-1.5;.4;0;];
A(:,:,16) = [-1.5;.5;0;];
A(:,:,17) = [-1.5;-.9;0;];
A(:,:,18) = [-1.5;-.8;0;];
A(:,:,19) = [-1.5;-.7;0;];
A(:,:,20) = [-1.5;-.6;0;];
A(:,:,21) = [-1.5;0;0;];
A(:,:,22) = [-1.5;-.1;0;];
A(:,:,23) = [-1.5;-.2;0;];
A(:,:,24) = [-1.5;-.3;0;];
A(:,:,25) = [-1.5;-.4;0;];
A(:,:,26) = [-1.5;-.5;0;];
A(:,:,27) = [-1.5;-1;0;];
A(:,:,28) = [-1.5;1.5;0;];
A(:,:,29) = [-1.5;1.4;0;];
A(:,:,30) = [-1.5;1.3;0;];
A(:,:,31) = [-1.5;1.2;0;];
A(:,:,32) = [-1.5;1.1;0;];


 
  for i = 1:32
      fence = translate(A(:,:,i))*(R1*((R*(S6*P))));
    draw(fence, 'b');
  end

A(:,:,33) = [.5;-1.5;0;];
A(:,:,34) = [.5;-1.4;0;];
A(:,:,35) = [.5;-1.3;0;];
A(:,:,36) = [.5;-1.2;0;];
A(:,:,37) = [.5;-1.1;0;];
A(:,:,38) = [.5;1;0;];
A(:,:,39) = [.5;.9;0;];
A(:,:,40) = [.5;.8;0;];
A(:,:,41) = [.5;.7;0;];
A(:,:,42) = [.5;.6;0;];
A(:,:,43) = [.5;0;0;];
A(:,:,44) = [.5;.1;0;];
A(:,:,45) = [.5;.2;0;];
A(:,:,46) = [.5;.3;0;];
A(:,:,47) = [.5;.4;0;];
A(:,:,48) = [.5;.5;0;];
A(:,:,49) = [.5;-.9;0;];
A(:,:,50) = [.5;-.8;0;];
A(:,:,51) = [.5;-.7;0;];
A(:,:,52) = [.5;-.6;0;];
A(:,:,53) = [.5;0;0;];
A(:,:,54) = [.5;-.1;0;];
A(:,:,55) = [.5;-.2;0;];
A(:,:,67) = [.5;-.3;0;];
A(:,:,68) = [.5;-.4;0;];
A(:,:,69) = [.5;-.5;0;];
A(:,:,70) = [.5;-1;0;];
A(:,:,71) = [.5;1.5;0;];
A(:,:,72) = [.5;1.4;0;];
A(:,:,73) = [.5;1.3;0;];
A(:,:,74) = [.5;1.2;0;];
A(:,:,75) = [.5;1.1;0;];

for i = 44:75
    fence = translate(A(:,:,i))*(R1*((R*(S6*P))));
    draw(fence, 'b');
end


%Plot the house
draw(Left,'y');
draw(Right,'y');
draw(Front, 'y');
draw(Back, 'y');
draw(Door1, 'r');
% draw(Door2, 'r');
